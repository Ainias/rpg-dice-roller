{
  "name": "rpg-dice-roller",
  "description": "A dice roller that accepts standard dice notation",
  "author": "GreenImp <info@greenimp.co.uk> (http://greenimp.co.uk",
  "version": "3.0.1",
  "license": "MIT",
  "main": "lib/es5/bundle.js",
  "module": "src/index.js",
  "browser": "lib/es5/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GreenImp/rpg-dice-roller.git"
  },
  "bugs": {
    "url": "https://github.com/GreenImp/rpg-dice-roller/issues"
  },
  "homepage": "https://github.com/GreenImp/rpg-dice-roller",
  "keywords": [
    "dice",
    "roller",
    "notation",
    "rpg",
    "roll",
    "pathfinder",
    "d&d"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "abab": "^2.0.0",
    "babel-loader": "^8.0.5",
    "jasmine": "^3.4.0",
    "jshint": "^2.10.2",
    "jshint-loader": "git+https://github.com/webpack-contrib/jshint-loader.git",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "xregexp": "^4.2.4"
  },
  "scripts": {
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "webpack --config-name umd",
    "build:es": "webpack --config-name es",
    "build:dev": "webpack --env.development --config-name es",
    "start": "webpack-dev-server --color --progress",
    "watch": "webpack --env.development --config-name es --watch",
    "test": "jshint src/ & jasmine"
  }
}
